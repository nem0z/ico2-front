{
    "version": 2,
    "builds": [
      {
        "src": "src/index.html",
        "use": "@vercel/static-build",
        "config": { "zeroConfig": true }
      }
    ],
    "routes": [
      {
        "src": "/ico/data/(.*)",
        "dest": "/data/$1"
      },
      {
        "src": "/ico/src/(.*)",
        "dest": "/src/$1"
      },
      {
        "src": "/ico/(.*)",
        "dest": "/$1"
      }
    ]
  }  